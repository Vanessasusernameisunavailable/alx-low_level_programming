#include <stdlib.h>
#include <time.h>
#include <stdio.h>

def main(void):
 * main - Prints a random number and states whether
 *         it is positive, negative, or zero.
 *
 *Return : Always 0 (Success)
 *\
int main(void)
 {
int n ;

srand(time(0));

n = rand() %  RAND_MAX ;
if (n > 0)
printf(" , is positive\n ")
else if (n == 0)
printf(" , is zero\n")
else
printf(" , is negative\n ");
return (0);
}
